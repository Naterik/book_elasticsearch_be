{
  "name": "book_nodejs",
  "version": "1.0.0",
  "description": "project for thesis",
  "license": "ISC",
  "author": "khuong",
  "type": "commonjs",
  "main": "dist/app.js",
  "scripts": {
    "start": "nodemon",
    "dev": "nodemon",
    "start:debug": "nodemon --inspect=9229 -e ts,tsx --exec node -r ts-node/register ./src/app.ts"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts",
    "ignore": [
      "node_modules"
    ],
    "exec": "ts-node ./src/app.ts"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^9.1.1",
    "express": "^5.1.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/node": "^24.4.0",
    "dotenv": "^17.2.2",
    "nodemon": "^3.1.10",
    "prisma": "^6.16.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  }
}
